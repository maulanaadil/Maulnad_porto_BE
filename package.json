{
  "name": "starter-template-express",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "_moduleAliases": {
    "@": "dist"
  },
  "scripts": {
    "build": "esbuild ./src/server.ts --bundle --platform=node --outfile=build/server.js",
    "start": "node dist/server.js",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon ./src/server.ts\"",
    "seed": "ts-node prisma/seed.ts",
    "lint-fix": "eslint \"**/*.{js,jsx}\" --fix",
    "precommit": "lint-staged",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^4.3.1",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
    "esbuild": "^0.15.15",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.5-lts.1",
    "swagger-jsdoc": "^6.2.5",
    "swagger-ui-express": "^4.6.0",
    "uuid": "^9.0.0",
    "zod": "^3.19.1",
    "zod-form-data": "^1.2.1"
  },
  "lint-staged": {
    "src/**/*.{jsx, js, json, css,html}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.7.18",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "concurrently": "^7.4.0",
    "eslint": "^8.23.1",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.3",
    "module-alias": "^2.2.2",
    "nodemon": "^2.0.20",
    "prisma": "^4.3.1",
    "typescript": "^4.8.3"
  }
}
